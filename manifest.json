{
    "name": "Pomodoro Timer",
    "description": "A customizable pomodoro timer",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "notifications"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
}